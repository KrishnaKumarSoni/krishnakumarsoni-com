{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ],
    "env": {
        "TRACKER_SCRIPT_NAME": "script.js"
    },
    "regions": ["iad1"],
    "functions": {
        "app.py": {
            "includeFiles": "config/**"
        }
    }
} 